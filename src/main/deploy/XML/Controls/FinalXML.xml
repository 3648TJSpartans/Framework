<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<robot>
<controller id="DRIVE" port="0" type="LOGITECH">
<CHASSIS turning_gain="0.5" type="TANK">
<axis axis="LEFT_TRIGGER" function="MODE_SLOW"/>
<axis axis="RIGHT_TRIGGER" function="MODE_FAST"/>
<axis axis="RIGHT_JOYSTICK_Y" function="RIGHT_SPEED"/>
<axis axis="LEFT_JOYSTICK_Y" function="LEFT_SPEED"/>
<button button="Y" function="CAMERA_AIM"/>
</CHASSIS>
<CLIMBER>
<button button="B" function="EXTEND"/>
<button button="A" function="RETRACT"/>
</CLIMBER>
<INTAKE>
<button button="RIGHT_SHOULDER" function="INTAKE_IN"/>
<button button="LEFT_SHOULDER" function="INTAKE_OUT"/>
</INTAKE>
</controller>
</robot>

<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<robot>
<CHASSIS fast="1" normal="0.8" slow="0.6">
<motor controller="SPARK" id="LEFT_SIDE" port="1"/>
<motor controller="SPARK" id="RIGHT_SIDE" port="2"/>
<!--<motor id="LEFT_SIDE_FRONT" controller="TALON" port="8"></motor>
<motor id="LEFT_SIDE_BACK" controller="TALON" port="9"></motor>
<motor id="RIGHT_SIDE_FRONT" controller="TALON" port="0"></motor>
<motor id="RIGHT_SIDE_BACK" controller="TALON" port="1"></motor>-->
</CHASSIS>
<SHOOTER>
<motor controller="SPARK" id="shooter" port="3"/>
</SHOOTER>
</robot>