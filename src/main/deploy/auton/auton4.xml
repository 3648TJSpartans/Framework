<?xml version="1.0"?>

<robot>
    <auton id="auton4" nextstep="start">
        <SequentialCommandGroup id="start" firststep="calibrateGyroX">
        <command type="ADIS16470_Set" id="calibrateGyroX" nextstep="drive" axis="kX" />
          <command type="SwerveDrive_Set" id="drive" subsystemID="SwerveDrive" nextstep="scoreHigh" 
            xTranslation ="0" yTranslation = "0" heading="0" setMaxSpeed = "1" setMaxAcceleration = "1" timeout=".3" autonPath="scorePath"/> 
            <command type="scorePos" autoType="score_high_auto" id="scoreHigh" nextstep="wristDown" armSubsystemID="arm" armChainSubsystemID="armChain" wristSubsystemID="wrist"/>
             <command type="Motor_Set" subsystemID="wrist" setPosition=".68" id="wristDown" nextstep="delay1"/>
            <command type="Util_Delay" delayLength="1" id="delay1" nextstep="clawOpen"/>
            <command subsystemID="claw" type="Motor_Set" setPower="-1" id="clawOpen" nextstep="delay2"/>
            <command type="Util_Delay" delayLength=".3" id="delay2" nextstep="clawClosed"/> 
             <command type="Motor_Set" subsystemID="claw" setPower="0" id="clawClosed" nextstep="transport"/>
            <command type="scorePos" autoType="transport_auto" id="transport" nextstep="delay3" armSubsystemID="arm" armChainSubsystemID="armChain" wristSubsystemID="wrist"/>
            <command type="Util_Delay" delayLength="2" id="delay3" nextstep="driveCharging"/>
            <command type="SwerveDrive_Set" id="driveCharging" subsystemID="SwerveDrive" nextstep="balance" 
            setMaxSpeed = "1" setMaxAcceleration = "1" timeout="3" autonPath="Path2"/> 
            <command type="SwerveDrive_Balance" subsystemID="SwerveDrive" timeout="4" id="balance" deadzone="0.015" kP="0.0075" nextstep="calibrateGyroZ"/>
            <command type="ADIS16470_Set" id="calibrateGyroZ" axis="kZ"/>
            
        </SequentialCommandGroup>
    </auton>
</robot>    