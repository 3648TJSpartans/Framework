                    kMaxOutput="1.0" distance_per_pulse="6.283185307179586" />
            </motor>
        </module>

        <module id="front_left" angularOffset="-1.5707963267948966">
            <motor id="drive" controller="sparkmax" port="6">
                <encoder id="driveEncoder" port="encoder" distance_per_pulse="0.05077956125529683" />
                <pid id="PID" encoderPort="encoder" kp="0.04" kf="0.2081708518741928" kMinOutput="-1.0" kMaxOutput="1.0"
                    distance_per_pulse="0.05077956125529683" />
            </motor>
            <motor id="turn" controller="sparkmax" port="5">
                <encoder id="turnEncoder" port="data" inverted="true"
                    distance_per_pulse="0.0008463260209216138" />
                <pid id="PID" kp="1" encoderPort="data" setPositionPIDWrappingEnabled="true"
                    setPositionPIDWrappingMinInput="0"
                    setPositionPIDWrappingMaxInput="6.283185307179586" kMinOutput="-1.0"
                    kMaxOutput="1.0" distance_per_pulse="6.283185307179586" />
            </motor>
        </module>

        <module id="back_left" angularOffset="3.141592653589793">
            <motor id="drive" controller="sparkmax" port="8">
                <encoder id="driveEncoder" port="encoder" distance_per_pulse="0.05077956125529683" />
                <pid id="PID" kp="0.04" encoderPort="encoder" kf="0.2081708518741928" kMinOutput="-1.0" kMaxOutput="1.0"
                    distance_per_pulse="0.05077956125529683" />
            </motor>
            <motor id="turn" controller="sparkmax" port="7">
