                    distance_per_pulse="0.05077956125529683" />
            </motor>
            <motor id="turn" controller="sparkmax" port="3">
                <encoder id="turnEncoder" port="data" inverted="true"
                    distance_per_pulse="0.0008463260209216138" />
                <pid id="PID" kp="1" encoderPort="data" setPositionPIDWrappingEnabled="true"
                    setPositionPIDWrappingMinInput="0"
