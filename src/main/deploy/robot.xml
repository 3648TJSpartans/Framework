<subsystem type="SwerveDrive" id="SwerveDrive" width="21" wheel_base="25.5" maxSpeedMetersPerSecond="4.8" maxAngularSpeed=" 6.283185307179586" fieldOriented="true">
        
        <!-- < // add theta/turning pid, xPid/ yPid >> -->
        <module id="front_right" angularOffset="0">
            <motor id="drive" controller="sparkmax" port="2">
                <encoder id="driveEncoder" port="encoder" distance_per_pulse="0.05077956125529683" />
                <pid id="PID" kp="0.04" encoderPort="encoder" kf="0.2081708518741928" kMinOutput="-1.0" kMaxOutput="1.0"
                    distance_per_pulse="0.05077956125529683" />
            </motor>
            <motor id="turn" controller="sparkmax" port="1">
                <encoder id="turnEncoder" port="data" inverted="true" distance_per_pulse="0.0008463260209216138"/>
                <pid id="PID" kp="1" encoderPort="data" setPositionPIDWrappingEnabled="true"
                    setPositionPIDWrappingMinInput="0"
                    setPositionPIDWrappingMaxInput="6.283185307179586" kMinOutput="-1.0"
                    kMaxOutput="1.0" distance_per_pulse="6.283185307179586" />
            </motor>
        </module>

        <module id="back_right" angularOffset="1.5707963267948966">
            <motor id="drive" controller="sparkmax" port="4">
                <encoder id="driveEncoder" port="encoder" distance_per_pulse="0.05077956125529683" />
