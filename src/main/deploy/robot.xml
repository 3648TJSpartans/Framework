<?xml version="1.0"?>

<robot>
   <subsystem type="TankDrive" id="TankDrive">
        <motorgroup id="left">
            <motor id="left1" side="left" controller="sparkpwm" port="0" />
        </motorgroup>
        <motorgroup id="right">
            <motor id="right1" side="right" controller="sparkpwm" port="1" />
        </motorgroup>
    </subsystem>
    <subsystem type="Motor" id="shooter">
        <motor id="m_shooter" controller="sparkpwm" port="2"/>   
        
    </subsystem>
    <subsystem type="Motor" id="aim">
        <motor id="m_aim" controller="sparkpwm" port="3"/>
    </subsystem>
</robot>
